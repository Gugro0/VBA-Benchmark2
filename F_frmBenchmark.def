Option Compare Database
Option Explicit



Private Sub Form_Open(Cancel As Integer)
  Dim objBench As New clsBenchmark
  
  Me.ctlClasses.RowSource = GetBenchmarkClasses
  Me.ctlIterations = objBench.MinIterations
  Me.ctlTimeout.Value = objBench.Timeout
End Sub

Private Sub cmdRun_Click()
  If Nz(Me.ctlClasses) <> "" Then
    Me.ctlResult = ""
    DoCmd.Hourglass True
    With Benchmark
      Me.ctlResult = .Run(Me.ctlClasses, Nz(Me.ctlIterations), Nz(Me.ctlTimeout)).Report
    End With
    DoCmd.Hourglass False
    Me.SetFocus
  End If
End Sub

Private Function GetBenchmarkClasses() As String
  Dim v
  Dim str As String
  
  For Each v In CurrentProject.AllModules
    If Right(v.Name, 5) = "Bench" Then str = str & v.Name & ";"
  Next
  GetBenchmarkClasses = str
End Function

Private Sub cmdHelp_Click()
  DoCmd.OpenForm "frmBenchmarkHelp", WindowMode:=acDialog
End Sub
