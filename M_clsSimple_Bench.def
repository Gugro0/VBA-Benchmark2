Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Private Const ITERATIONS As Integer = 1000

Public Sub IntegerBenchmark()
  Dim i As Integer
  
  While i < ITERATIONS
    i = i + 1
  Wend
End Sub

Public Sub LongBenchmark()
  Dim i As Long
  
  While i < ITERATIONS
    i = i + 1
  Wend
End Sub

Public Sub DoubleBenchmark()
  Dim i As Double
  
  While i < ITERATIONS
    i = i + 1#
  Wend
End Sub

Public Sub CurrencyBenchmark()
  Dim i As Currency
  
  While i < ITERATIONS
    i = i + 1
  Wend
End Sub