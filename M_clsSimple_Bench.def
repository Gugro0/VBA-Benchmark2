Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Private Const ITERATIONS As Long = 20000

Public Sub IntegerBenchmark()
  Dim i As Integer
  Dim max As Integer
  
  max = ITERATIONS
  While i < max
    i = i + 1
  Wend
End Sub

Public Sub LongBenchmark()
  Dim i As Long
  Dim max As Long
  
  max = ITERATIONS
  While i < max
    i = i + 1
  Wend
End Sub

Public Sub DoubleBenchmark()
  Dim i As Double
  Dim max As Double
  
  max = ITERATIONS
  While i < max
    i = i + 1#
  Wend
End Sub

Public Sub CurrencyBenchmark()
  Dim i As Currency
  Dim max As Currency
  
  max = ITERATIONS
  While i < max
    i = i + 1
  Wend
End Sub